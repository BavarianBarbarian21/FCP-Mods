<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Protectron: Walking humanoid robot with stiff mechanical parts -->
  <BodyDef>
    <defName>Protectron</defName>
    <label>protectron</label>
    <corePart>
      <def>MechanicalTorso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>MechanicalNeck</def>
          <coverage>0.10</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>MechanicalHead</def>
              <coverage>0.80</coverage>
              <parts>
                <li>
                  <def>ArtificialBrain</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>left shoulder</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>left arm</customLabel>
              <coverage>0.85</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>right shoulder</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>right arm</customLabel>
              <coverage>0.85</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!-- Handy: Hovering spherical robot with three eyes and three arms -->
  <BodyDef>
    <defName>Handy</defName>
    <label>handy</label>
    <corePart>
      <def>MechanicalSphere</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>Thruster</def>
          <coverage>0.15</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>MechanicalEye</def>
          <customLabel>top eye</customLabel>
          <coverage>0.10</coverage>
        </li>
        <li>
          <def>MechanicalEye</def>
          <customLabel>left eye</customLabel>
          <coverage>0.10</coverage>
        </li>
        <li>
          <def>MechanicalEye</def>
          <customLabel>right eye</customLabel>
          <coverage>0.10</coverage>
        </li>
        <li>
          <def>MechanicalArm</def>
          <customLabel>welding tool arm</customLabel>
          <coverage>0.15</coverage>
        </li>
        <li>
          <def>MechanicalArm</def>
          <customLabel>gripper arm</customLabel>
          <coverage>0.15</coverage>
        </li>
        <li>
          <def>MechanicalArm</def>
          <customLabel>multi-tool arm</customLabel>
          <coverage>0.15</coverage>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!-- Robobrain: Rolling on tracks, braintank, two crude arms -->
  <BodyDef>
    <defName>Robobrain</defName>
    <label>robobrain</label>
    <corePart>
      <def>MechanicalChassis</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>MechanicalTracks</def>
          <coverage>0.30</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>Braintank</def>
          <coverage>0.20</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>ArtificialBrain</def>
              <coverage>0.10</coverage>
              <depth>Inside</depth>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalArm</def>
          <customLabel>left crude arm</customLabel>
          <coverage>0.15</coverage>
        </li>
        <li>
          <def>MechanicalArm</def>
          <customLabel>right crude arm</customLabel>
          <coverage>0.15</coverage>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!-- Eyebot: Floating spherical robot with antennas and a single eye -->
  <BodyDef>
    <defName>Eyebot</defName>
    <label>eyebot</label>
    <corePart>
      <def>MechanicalSphere</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>Thruster</def>
          <coverage>0.15</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Antenna</def>
          <customLabel>left antenna</customLabel>
          <coverage>0.05</coverage>
        </li>
        <li>
          <def>Antenna</def>
          <customLabel>right antenna</customLabel>
          <coverage>0.05</coverage>
        </li>
        <li>
          <def>OpticalSensor</def>
          <customLabel>eye sensor</customLabel>
          <coverage>0.20</coverage>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!-- Assaultron: Sleeker, more humanlike robot -->
  <BodyDef>
    <defName>Assaultron</defName>
    <label>assaultron</label>
    <corePart>
      <def>MechanicalTorso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>MechanicalNeck</def>
          <coverage>0.10</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>AdvancedMechanicalHead</def>
              <coverage>0.80</coverage>
              <parts>
                <li>
                  <def>ArtificialBrain</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>OpticalSensor</def>
                  <customLabel>advanced eye sensor</customLabel>
                  <coverage>0.15</coverage>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>left shoulder</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>left arm</customLabel>
              <coverage>0.85</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>right shoulder</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>right arm</customLabel>
              <coverage>0.85</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
      </parts>
    </corePart>
  </BodyDef>

  <!-- Robodog: Dog with mixed robotic/canine legs and braintank -->
  <BodyDef>
    <defName>Robodog</defName>
    <label>robodog</label>
    <corePart>
      <def>Torso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>Leg</def>
          <customLabel>front right robotic leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>RoboticFoot</def>
              <customLabel>front right robotic foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>front left canine leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>CanineFoot</def>
              <customLabel>front left canine foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>rear right canine leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>CanineFoot</def>
              <customLabel>rear right canine foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>rear left robotic leg</customLabel>
          <coverage>0.14</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>RoboticFoot</def>
              <customLabel>rear left robotic foot</customLabel>
              <coverage>0.1</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Neck</def>
          <coverage>0.075</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>DogHead</def>
              <coverage>0.80</coverage>
              <parts>
                <li>
                  <def>Braintank</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
              </parts>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
    <!-- Sentrybot: Three roller legs, bulky humanoid torso, two arms, head -->
    <BodyDef>
      <defName>Sentrybot</defName>
      <label>sentrybot</label>
      <corePart>
        <def>BulkyMechanicalTorso</def>
        <height>Middle</height>
        <depth>Outside</depth>
        <parts>
          <li>
            <def>RollerLeg</def>
            <customLabel>front roller leg</customLabel>
            <coverage>0.13</coverage>
            <height>Bottom</height>
          </li>
          <li>
            <def>RollerLeg</def>
            <customLabel>left rear roller leg</customLabel>
            <coverage>0.13</coverage>
            <height>Bottom</height>
          </li>
          <li>
            <def>RollerLeg</def>
            <customLabel>right rear roller leg</customLabel>
            <coverage>0.13</coverage>
            <height>Bottom</height>
          </li>
          <li>
            <def>MechanicalShoulder</def>
            <customLabel>left shoulder</customLabel>
            <coverage>0.17</coverage>
            <parts>
              <li>
                <def>MechanicalArm</def>
                <customLabel>left arm</customLabel>
                <coverage>0.85</coverage>
              </li>
            </parts>
          </li>
          <li>
            <def>MechanicalShoulder</def>
            <customLabel>right shoulder</customLabel>
            <coverage>0.17</coverage>
            <parts>
              <li>
                <def>MechanicalArm</def>
                <customLabel>right arm</customLabel>
                <coverage>0.85</coverage>
              </li>
            </parts>
          </li>
          <li>
            <def>MechanicalNeck</def>
            <coverage>0.10</coverage>
            <height>Top</height>
            <parts>
              <li>
                <def>MechanicalHead</def>
                <coverage>0.80</coverage>
                <parts>
                  <li>
                    <def>ArtificialBrain</def>
                    <coverage>0.10</coverage>
                    <depth>Inside</depth>
                  </li>
                </parts>
              </li>
            </parts>
          </li>
        </parts>
      </corePart>
    </BodyDef>

</Defs>
